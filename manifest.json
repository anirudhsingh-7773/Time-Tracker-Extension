{
  "manifest_version": 3,
  "name": "Hello World Extension",
  "version": "1.0",
  "description": "Hello World only on Keka Attendance Logs page",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"],
  "host_permissions": [
    "https://innoraft.keka.com/*"
  ]
}
